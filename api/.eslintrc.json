{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "AppError": true
  },
  // "extends": ["eslint:recommended", "google"],
  // "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "require-jsdoc": 0,
    "no-invalid-this": 0,
    "comma-dangle": 1,
    "object-curly-spacing": ["error", "always"]
  }
}